pipeline{
    agent any
    stages{
        stage('GIt Checkout'){
            steps{
                git credentialsId: 'Github_credentials1', url: 'https://github.com/Test11411/myweb'
                echo "Git Checkout Successful ...."
            }
       }
       stage('Maven Build'){
       steps{
       sh "mvn clean package"
           }
       }
    }
}
