pipeline{
    agent any
        stages{
            stage("Git Checkout"){
                steps{
                    git credentialsId: 'Git_credentials', url: 'https://github.com/Test11411/myweb'
		            echo "Git checkout is successful"
                }
            }
            stage("Maven Build"){
                steps{
                    sh "mvn clean package"
                }
            }
        }
    }
